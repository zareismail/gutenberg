{
    "name": "zareismail/gutenberg",
    "description": "The Laravel Nova Interaction tool with the Cypress.",
    "keywords": [
        "laravel",
        "nova",
        "cypress",
        "gutenberg"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/many-to-many": "~0.4",
        "whitecube/nova-flexible-content": "~0.2",
        "zareismail/markable": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Gutenberg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Gutenberg\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
